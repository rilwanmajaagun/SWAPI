{
  "name": "swapi",
  "version": "1.0.0",
  "description": "Api endpoint for listing names of Star Wars movies, adding and listing anonymous comments for a movie, and getting the character list for a movie.",
  "main": "index.js",
  "nyc": {
    "all": false,
    "include": [
      "src/**"
    ],
    "exclude": [
      "**/migrations/**",
      "src/config/**"
    ]
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate": "^1.0.0-beta.16",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "dotenv-safe": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "file-stream-rotator": "^0.5.7",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.1.3",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nock": "^13.2.0",
    "nodemon": "^2.0.14",
    "nyc": "^15.1.0",
    "pg-promise": "^10.11.1",
    "prettier": "^2.4.1",
    "request-ip": "^2.1.3",
    "supertest": "^6.1.6",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "scripts": {
    "pretest": "npm run migrate-test:down && npm run migrate-test:up",
    "test": "cross-env SWAPI_NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=text-summary mocha -r esm test --exit --timeout=150000 ",
    "posttest": "npm run migrate-test:down",
    "dev": "nodemon",
    "prestart": "db-migrate up --config database.json -e development",
    "start": "node -r esm src/index.js",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down -c 100",
    "migrate-test:up": "db-migrate up --config database.json -e test",
    "migrate-test:down": "db-migrate down -c 100 --config database.json -e test",
    "lint": "eslint . --ext .js ./",
    "lint-fix": "eslint . --ext .js ./ --fix",
    "prettier:base": "prettier --config .prettierrc 'src/**/*.js' --write",
    "coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=text-summary mocha --timeout 100000 -r esm 'test' --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rilwanmajaagun/SWAPI.git"
  },
  "author": "rilwan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rilwanmajaagun/SWAPI/issues"
  },
  "homepage": "https://github.com/rilwanmajaagun/SWAPI#readme",
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2"
  },
  "directories": {
    "test": "test"
  },
  "keywords": []
}
